@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>AdminLTE 3 | @ViewData["Title"]</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/libs/bootstrap/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link href="~/libs/font-awesome/css/all.min.css" rel="stylesheet" />
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/libs/ionicons/css/ionicons.min.css">
    <!-- icheck bootstrap -->
    <link rel="stylesheet" href="~/libs/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/libs/admin-lte/css/adminlte.min.css">

</head>
<body class="hold-transition login-page" >
    <div class="login-box">
        <!-- /.login-logo -->
        <div class="card card-outline card-primary">
            <div class="card-header text-center">
                <a href="/" class="h1"><b>Admin</b>LTE</a>
            </div>
            @RenderBody()
        </div>
        <!-- /.card -->
    </div>
    <!-- /.login-box -->

    <!-- /.register-box -->
    <!-- jQuery -->
    <script src="~/libs/jquery/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/libs/bootstrap/js/bootstrap.min.js"></script>

</body>
</html>

